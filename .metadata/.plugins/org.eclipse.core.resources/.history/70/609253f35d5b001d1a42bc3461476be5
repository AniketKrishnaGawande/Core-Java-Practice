package fruitBasket;

import java.util.Iterator;
import java.util.List;

public class Numbers {
	
	
	public <T extends Number & Comparable<T>> T findmaxno(List<T> list)
	{
		Iterator<T> itr = list.iterator();
		T temp=null;
		while(itr.hasNext())
		{	
			if(((T)list).compareTo(temp)>0)
			{
				temp=itr.next();
			}
		}
		
		
		
		
		return null;
	}

}
